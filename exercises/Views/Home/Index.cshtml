                                        </div>
                                    </div>
                                    <div class="row" style="margin-top:5px;">
                                        <div class="col-md-12">
                                            <button id="showPalindromeCode">Show Code</button>
                                        </div>
                                    </div>
                                    <div id="codePalindrome" class="row">
                                        <pre class="brush: js">
                                        $("#btn3").click(function () {
                                         var someText = $("#sometext").val();
                                         var checkForPalindrome = someText.split('').reverse().join('');
                                         if (someText === checkForPalindrome) {
                                         $("#palindrome").text("The word is a palindrome!")
                                         } else {
                                         $("#palindrome").text("The word is not a palindrome!")
                                         }
                                        })
                                        </pre>
                                    </div>
                                </div>
                                <div class="col-md-5">
                                    <div id="palindrome"></div>
                                </div>
                            </div>
                        </div>
                    </div>
