                        <div id="Fizzbuzz" class="tab-pane fade">
                            <h3>FizzBuzz</h3>
                            <div class="row">
                                <div class="col-md-7">
                                    <div class="row">
                                        <div class="col-md-6"> Please enter num1</div>
                                        <div class="col-md-6"><input id="number1" type="number" name="name" value="" /></div>
                                    </div>
                                    <div class="row">
                                        <div class="col-md-6">  Please enter num2 </div>
                                        <div class="col-md-6">
                                            <input id="number2" type="number" name="name" value="" />
                                        </div>
                                    </div>
                                    <div class="row">
                                        <div class="col-md-12">
                                            <button id="btn1"> Calculate</button>
                                        </div>
                                    </div>
                                    <div class="row" style="margin-top:5px;">
                                        <div class="col-md-12">
                                            <button id="showFizzBuzzCode">Show Code</button>
                                        </div>
                                    </div>
                                    <div id="codeFizzBuzz" class="row">
                                        <pre class="brush: js">
                                         $("#btn1").click(function () {
                                          var number1 = Number($("#number1").val());
                                          var number2 = Number($("#number2").val());
                                          var numbers = [];
                                            for (var loop = 1; loop <= 100; loop++) {
                                             if (loop % number1 == 0 && loop % number2 == 0) {
                                              numbers.push("FIZZBUZZ")
                                                  } else if (loop % number1 == 0) {
                                                     numbers.push("FIZZ")
                                              } else if (loop % number2 == 0) {
                                                     numbers.push("BUZZ")
                                              } else {
                                                     numbers.push(loop);
                                              }
                                           }
                                          var output = numbers.join(' , ');
                                          $("#fizzbuzz").text(output);
                                        })
                                        </pre>
                                    </div>
                                </div>
                               
                                <div class="col-md-5">
                                    <div id="fizzbuzz"></div>
                                </div>
                            </div>
