                        <div id="Factorial" class="tab-pane fade">
                            <h3>Factorial</h3>
                            <div class="row">
                                <div class="col-md-7">
                                    <div class="row">
                                        <div class="col-md-6">Please enter a number</div>
                                        <div class="col-md-6">
                                            <input id="num" type="number" name="name" value="" />
                                        </div>
                                    </div>
                                    <div class="row">
                                        <div class="col-md-12">
                                            <button id="btnfact">Calculate</button>
                                        </div>
                                    </div>
                                    <div class="row" style="margin-top:5px;">
                                        <div class="col-md-12">
                                            <button id="showFactorialCode">Show Code</button>
                                        </div>
                                    </div>
                                    <div id="codeFactorial" class="row">
                                        <pre class="brush: js">
                                          $("#btnfact").click(function () {
                                          var num = Number($("#num").val());
                                          var fact = 1;
                                          for (var i = num; i >= 1; i--) {
                                             fact = fact * i;
                                            }
                                          var result = fact;
                                          $("#factorial").text("The factorial is " + result);
                                          })
                                        </pre>
                                    </div>
                                </div>

                                <div class="col-md-5">
                                    <div id="factorial"></div>
                                </div>
                            </div>
                        </div>
                        <div id="Palindrome" class="tab-pane fade">
